# Loading work.elevenBitUpCount(fast)
add wave -position insertpoint  \
sim:/elevenBitUpCount/Enable \
sim:/elevenBitUpCount/Clock \
sim:/elevenBitUpCount/Reset \
sim:/elevenBitUpCount/Done
force -freeze sim:/elevenBitUpCount/Enable 1'h1 0
force -freeze sim:/elevenBitUpCount/Clock 1 0, 0 {50 ns} -r 100
force -freeze sim:/elevenBitUpCount/Reset 1'h1 0
run 20
force -freeze sim:/elevenBitUpCount/Reset 1'h0 0
run 20
run 20000
run 2000000
quit -sim
